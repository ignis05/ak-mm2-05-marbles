{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended"],
	"jsRules": {},
	"rules": {
		"quotemark": false,
		"semicolon": false,
		"indent": [false],
		"comment-format": false,
		"no-console": false,
		"curly": false,
		"no-var-requires": false,
		"variable-name": false,
		"arrow-parens": false,
		"triple-equals": false,
		"member-ordering": false,
		"one-line": false,
		"prefer-const": false,
		"max-line-length": false
	},
	"rulesDirectory": []
}
